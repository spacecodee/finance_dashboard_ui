            minWidth="-Infinity" nodeOrientation="LEFT_TO_RIGHT" text="Text" wrapText="true" HBox.hgrow="ALWAYS">
        <styleClass>
            <String fx:value="font-bold"/>
            <String fx:value="button-sidebar-pressed"/>
        </styleClass>
        <graphic>
            <ImageView cache="true" cacheHint="QUALITY" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true"
                       preserveRatio="true">
                <Image url="@../../../assets/icons/cards_blue.png"/>
