<?import javafx.scene.layout.HBox?>

<HBox fx:id="btnParent" alignment="CENTER" cache="true" cacheHint="QUALITY" maxHeight="-Infinity"
      maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.spacecodee.finance_dashboard.controller.components.buttons.ButtonIconLine">
    <cursor>
        <Cursor fx:constant="HAND"/>
    </cursor>
    <stylesheets>
        <URL value="@../../../assets/css/fonts.css"/>
        <URL value="@../../../assets/css/styles.css"/>
    </stylesheets>
    <styleClass>
        <String fx:value="sidebar-body"/>
        <String fx:value="hb_button"/>
    </styleClass>
    <Button fx:id="btnBody" alignment="BASELINE_LEFT" cache="true" cacheHint="QUALITY" graphicTextGap="15.0"
