            <Cursor fx:constant="HAND"/>
        </cursor>
    </Button>
    <ImageView fx:id="imgLine" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true"
               HBox.hgrow="ALWAYS">
        <Image url="@../../../assets/icons/horizontal-line.png"/>
